{
  "manifest_version": 3,
  "name": "coze-beautify",
  "version": "1.0.0",
  "description": "this is a chrome extension named coze-beautify.",
  "permissions": ["storage"],
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": ["*://*/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.coze.com/space/*/bot/*",
        "*://*.coze.com/space/*/bot",
        "*://*.coze.com/explore/*"
      ],
      "js": ["src/pages/content/index.js"],
      "css": ["assets/css/contentStyle17043481545.chunk.css"]
    }
  ]
}
